<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Nom011App.VerificacionViews.General"  Icon="ic_settings">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Nueva Verificación"
                VerticalOptions="Center" 
                HorizontalOptions="Center" />
            <Picker x:Name="EquipoEntry"></Picker>
            <Entry Placeholder="Uso" x:Name="UsoEntry"></Entry>
            <Entry Placeholder="Latitud" x:Name="LatitudEntry"></Entry>
            <Entry Placeholder="Longitud" x:Name="LongitudEntry"></Entry>
            <Entry Placeholder="Este" x:Name="EsteEntry"></Entry>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>